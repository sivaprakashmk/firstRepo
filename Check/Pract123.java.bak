import java.util.Scanner;
class Pract123 
{
	public static void main(String[] args) 
	{
		Scanner s = new Scanner(System.in);
		System.out.println("enter a number");
		int n = s.nextInt();
		count(n);
	}
		public static void count(int n)
			{
				int co = 0, cone = 0;
				while(n!=0)
				{
					int d = n%10;
					if(d==0)
					{
						co++;
					}
					else
							cone++;
					n/=10;
					
						System.out.println(" the nunmber of co is :" +co);
						System.out.println("the number of cone is : " +cone);
				}
			}
		
	
}
